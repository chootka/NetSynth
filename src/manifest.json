{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "https://github.com/chootka/net-synth",
  "description": "Captures network traffic as OSC for sonification",
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "<all_urls>",
    "*://*/*",
    "webRequest"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/lib/Tone.min.js", 
      "js/lib/osc.min.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    // "default_popup": "popup.html",
    // "default_title": "__MSG_extName__",
    // "default_icon": {
    //   "19": "icons/19.png",
    //   "38": "icons/38.png"
    // }
  }
// ,
// "options_ui": {
//   "page": "options.html",
//   "browser_style": true
// }
}